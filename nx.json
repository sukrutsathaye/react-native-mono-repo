{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ],
      "cache": true
    },
    "test": {
      "dependsOn": [
        "^test"
      ],
      "outputs": [
        "{projectRoot}/coverage"
      ],
      "cache": true
    },
    "publish:github": {
      "dependsOn": [
        "^publish:github"
      ],
      "cache": true
    },
    "start": {
      "cache": true
    }
  },
  "defaultBase": "main",
  "nxCloudAccessToken": "YjU0MWVmZDctYzA5Ni00YTYxLWIzZjAtZTcwMTI1MjEyZGM0fHJlYWQtd3JpdGU="
}
